<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:width="28dp"
            android:height="8dp"
            android:viewportWidth="28"
            android:viewportHeight="8">
            <path
                android:name="one"
                android:fillAlpha="0"
                android:fillColor="#704C4C"
                android:fillType="nonZero"
                android:pathData="M 0 4 C 0 1.8 1.8 0 4 0 C 6.2 0 8 1.8 8 4 C 8 6.2 6.2 8 4 8 C 1.8 8 0 6.2 0 4" />
            <path
                android:name="two"
                android:fillAlpha="0"
                android:fillColor="#704C4C"
                android:fillType="nonZero"
                android:pathData="M 10 4 C 10 1.8 11.8 0 14 0 C 16.2 0 18 1.8 18 4 C 18 6.2 16.2 8 14 8 C 11.8 8 10 6.2 10 4" />
            <path
                android:name="three"
                android:fillAlpha="0"
                android:fillColor="#704C4C"
                android:fillType="nonZero"
                android:pathData="M 20 4 C 20 1.8 21.8 0 24 0 C 26.2 0 28 1.8 28 4 C 28 6.2 26.2 8 24 8 C 21.8 8 20 6.2 20 4" />
        </vector>
    </aapt:attr>

    <target android:name="one">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="250"
                    android:propertyName="fillAlpha"
                    android:valueFrom="1"
                    android:valueTo="0" />
                <objectAnimator
                    android:duration="250"
                    android:propertyName="fillAlpha"
                    android:valueFrom="0"
                    android:valueTo="1" />
            </set>
        </aapt:attr>
    </target>

    <target android:name="two">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:startOffset="50"
                    android:duration="350"
                    android:propertyName="fillAlpha"
                    android:valueFrom="1"
                    android:valueTo="0" />
                <objectAnimator
                    android:startOffset="100"
                    android:duration="350"
                    android:propertyName="fillAlpha"
                    android:valueFrom="0"
                    android:valueTo="1" />
            </set>
        </aapt:attr>
    </target>

    <target android:name="three">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:startOffset="150"
                    android:duration="450"
                    android:propertyName="fillAlpha"
                    android:valueFrom="1"
                    android:valueTo="0" />
                <objectAnimator
                    android:startOffset="200"
                    android:duration="450"
                    android:propertyName="fillAlpha"
                    android:valueFrom="0"
                    android:valueTo="1" />
            </set>
        </aapt:attr>
    </target>
</animated-vector>